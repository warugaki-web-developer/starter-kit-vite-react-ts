{
  "name": "starter-vite-react-ts",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:eslint": "eslint --ignore-path .gitignore . --ext .js,.jsx,.ts,.tsx",
    "fix:eslint": "eslint --fix --ignore-path .gitignore . --ext .js,.jsx,.ts,.tsx",
    "lint:stylelint": "stylelint --ignore-path .gitignore ./src/**/*.{css,jsx,tsx}",
    "fix:stylelint": "stylelint --fix --ignore-path .gitignore ./src/**/*.{css,jsx,tsx}",
    "lint:prettier": "prettier --check --ignore-path .gitignore \"!package-lock.json\" './**/*.{js,jsx,ts,tsx,json,css}'",
    "fix:prettier": "prettier --write --ignore-path .gitignore \"!package-lock.json\" './**/*.{js,jsx,ts,tsx,json,css}'",
    "lint:all": "run-s lint:eslint lint:stylelint lint:prettier",
    "fix:all": "run-s fix:eslint fix:stylelint fix:prettier"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.3",
    "@reduxjs/toolkit": "1.7.0-rc.0",
    "postcss": "^8.4.12",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^8.0.1",
    "sanitize.css": "^13.0.0",
    "webpack": "^5.72.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "@types/babel__core": "^7.1.19",
    "@types/react": "^18.0.7",
    "@types/react-dom": "^18.0.0",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vitejs/plugin-react": "^1.3.1",
    "babel-loader": "^8.2.5",
    "eslint": "^8.14.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.6.2",
    "stylelint": "^14.7.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.5"
  }
}
